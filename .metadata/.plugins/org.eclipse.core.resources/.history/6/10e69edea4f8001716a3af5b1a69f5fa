package main;

public class Piece 
{
	boolean player; //player 1 == true, player 2 == false
	Space space;
	
	public Piece(boolean play, Space location)
	{
		player = play;
		space = location;
	}
	
	public Space getSpace()
	{
		return space;
	}
	
	public boolean getPlayer()
	{
		return player;
	}
	
	public void setSpace(Space newSpace)
	{
		space = newSpace;
	}
}
